{
  "dependencies": [""],
  "gtk":
    {
      "gtk-theme-name": "Nordic",
      "gtk-icon-theme-name": "Papirus-Dark",
      "gtk-font-name": "Noto Sans 11",
      "gtk-cursor-theme-name": "capitaine-cursors",
      "gtk-cursor-theme-size": "0"
    },
  "qt":
    {
      "color_scheme_path": "/usr/share/color-schemes/Nordic.colors",
      "custom_palette": "true",
      "icon_theme": "Papirus-Dark",
      "standard_dialogs": "default",
      "style": "Breeze"
    },
  "i3wm":
    {
      "i3wm-theme": "i3-base16-nord.config"
    },
  "i3status":
    {
      "i3status-theme": "i3status-base16-nord.config",
      "color_good": "",
      "color_bad": "",
      "color_degraded": ""
    },
  "dunst":
    {
      "dunst-theme": "dunst-base16-nord.dunstrc",
      "frameColor": "#aaaaaa",
      "frameWidth": "0",
      "separatorColor": "auto",
      "urgencyLow-BackgroundColor": "#222222",
      "urgencyLow-ForegroundColor": "#888888",
      "urgencyNormal-BackgroundColor": "#285577",
      "urgencyNormal-ForegroundColor": "#ffffff",
      "urgencyCritical-BackgroundColor": "#900000",
      "urgencyCritical-ForegroundColor": "#ffffff"
    },
  "kitty":
    {
      "kitty-theme": "kitty-base16-nord-256.conf"
    },
  "xresources":
    {
      "xresources-theme": "xresources-base16-nord-256.Xresources"
    },
  "wallpaper":
    {
      "wallpaper-theme": "nord-theme-arch.png",
      "wallpaper-source" : ""
    }
}
