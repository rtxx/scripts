{
  "dependencies": ["qt5ct-kde", "breeze", "breeze-gtk", "papirus-icon-theme", "capitaine-cursors"],
  "gtk":
    {
      "gtk-theme-name": "Nordic-Polar",
      "gtk-icon-theme-name": "Papirus-Light",
      "gtk-font-name": "Noto Sans 11",
      "gtk-cursor-theme-name": "capitaine-cursors",
      "gtk-cursor-theme-size": "0"
    },
  "qt":
    {
      "color_scheme_path": "/usr/share/color-schemes/Breeze.colors",
      "custom_palette": "true",
      "icon_theme": "Papirus",
      "standard_dialogs": "default",
      "style": "Breeze"
    },
  "i3":
    {
      "i3-theme": "i3-base16-classic-light"
    },
  "i3status":
    {
      "i3status-theme": "i3status-base16-classic-light"
    },
  "dunst":
    {
      "dunst-theme": "auto"
    },
  "xresources": "base16-classic-light-256.Xresources",
  "wallpaper": "tomasz-smal-7NgQUco_yak-unsplash.jpg"
}


