{
  "dependencies": ["breeze","materia-gtk-theme","materia-kde","papirus-icon-theme","capitaine-cursors"],
  "gtk":
    {
      "gtk-theme-name": "Materia-light",
      "gtk-icon-theme-name": "Papirus",
      "gtk-font-name": "Noto Sans 11",
      "gtk-cursor-theme-name": "capitaine-cursors",
      "gtk-cursor-theme-size": "0"
    },
  "qt":
    {
      "color_scheme_path": "/usr/share/color-schemes/MateriaLight.colors",
      "custom_palette": "true",
      "icon_theme": "Papirus-Light",
      "standard_dialogs": "default",
      "style": "Breeze"
    },
  "i3wm":
    {
      "i3wm-theme": "i3-base16-materia.config"
    },
  "i3status":
    {
      "i3status-theme": "i3status-base16-materia.config",
      "color_good": "",
      "color_bad": "",
      "color_degraded": ""
    },
  "dunst":
    {
      "dunst-theme": "dunst-base16-materia.dunstrc",
      "frameColor": "#aaaaaa",
      "frameWidth": "0",
      "separatorColor": "auto",
      "urgencyLow-BackgroundColor": "#222222",
      "urgencyLow-ForegroundColor": "#888888",
      "urgencyNormal-BackgroundColor": "#285577",
      "urgencyNormal-ForegroundColor": "#ffffff",
      "urgencyCritical-BackgroundColor": "#900000",
      "urgencyCritical-ForegroundColor": "#ffffff"
    },
  "kitty":
    {
      "kitty-theme":"kitty-base16-materia-256.conf"
    },
  "xresources":
    {
      "xresources-theme": "xresources-base16-materia.Xresources"
    },
  "wallpaper":
    {
      "wallpaper-theme": "willian-justen-de-vasconcellos-T_Qe4QlMIvQ-unsplash.jpg",
      "wallpaper-source" : "https://unsplash.com/"
    }
}
